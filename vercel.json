{
    "version": 2,
    "builds": [
      {
        "src": "api/index.go",
        "use": "@vercel/go",
        "config": {
          "goBuildFlags": "-ldflags '-s -w'"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.go"
      }
    ],
    "build": {
      "env": {
        "GO_BUILD_FLAGS": "-ldflags '-s -w'"
      }
    }
  }
  